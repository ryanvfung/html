    | <![endif]-->
    +meta("author", "Ryan Fung")
    //- script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js")
    script(src="file:///C:/!/lib/jquery/jquery-1.11.2.min.js")
    //- script(src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js")
    //- script(src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha/js/bootstrap.min.js")
    script(src="file:///C:/!/lib/bootstrap/bootstrap-4.0.0-alpha-dist/js/bootstrap.min.js")
