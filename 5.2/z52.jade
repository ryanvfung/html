mixin meta(n, c)
  meta(name=n content=c)
mixin css(url)
  link(rel="stylesheet" type="text/css" href=url)

doctype html
// Ryan Fung's HTML Template v5.2
html(lang="en")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    +meta("viewport", "width=device-width, initial-scale=1")
